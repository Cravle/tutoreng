{
  "name": "tutoreng",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": "true",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "turbo:build": "turbo run build",
    "turbo:dev": "turbo run dev --parallel",
    "turbo:lint": "turbo run lint",
    "turbo:format": "turbo run format",
    "ui": "yarn workspace @tutoreng/ui run dev",
    "api": "yarn workspace @tutoreng/api run start",
    "db:build": "yarn workspace @tutoreng/db run build",
    "db:migrate": "yarn workspace @tutoreng/db run migrate:run"
  },
  "devDependencies": {
    "prettier": "^2.7.1",
    "turbo": "*",
    "typescript": "^4.8.4"
  }
}
